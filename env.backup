# Multi-Agent System Environment Variables

# OpenRouter для всех LLM моделей (кроме Yandex) 
OPENROUTER_API_KEY=test_key_replace_with_real

# Yandex API для голосовых задач
YANDEX_API_KEY=test_yandex_key
YANDEX_FOLDER_ID=test_folder_id

# Telegram Bot Token для пользовательского интерфейса
TELEGRAM_BOT_TOKEN=test_bot_token_replace_with_real

# Настройки системы
MAS_LOG_LEVEL=INFO
MAS_DEBUG=true
DEFAULT_MODEL=anthropic/claude-3-haiku
MAX_CHAT_ROUNDS=10
