# Example environment variables for MAS development
# Copy this file to `.env` and fill in real values before running

# API keys
OPENROUTER_API_KEY=
YA_GPT_KEY=
YA_SPEECHKIT_API_KEY=
YANDEX_FOLDER_ID=

# Telegram bot
TELEGRAM_TOKEN=

# n8n workflow service
N8N_URL=http://localhost:5678
N8N_API_KEY=

# Database settings for internal deployments
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=mas
POSTGRES_USER=mas
POSTGRES_PASSWORD=maspass

# Redis settings
REDIS_PORT=6379

# Variables used by compose.yml for client deployments
CLIENT_POSTGRES_USER=mas_client
CLIENT_POSTGRES_PASSWORD=masclientpass
CLIENT_POSTGRES_DB=mas_client
CLIENT_REDIS_PORT=6380
CLIENT_N8N_USER=admin
CLIENT_N8N_PASSWORD=admin
CLIENT_N8N_PORT=5679
